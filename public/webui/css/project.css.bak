.clear{
	clear:both
}
.shadow{
-moz-box-shadow:0px 0px 20px #111;
-webkit-box-shadow:0px 0px 20px #111;
box-shadow:0px 0px 20px #111
}

.bgwhite{background:#fff}
.bgbold{background:#333}
.bgodd{background:#ddd}
.bgeven{background:#eee}
.bgodd a,.bgeven a{color:#444}
.padding_search{padding:10px 8px 2px 0px}
.padding_10p{padding:10px}
.padding_15p{padding:15px}
.padding_20p{padding:20px}
.padding_30p{padding:30px}
.padding_10pv{padding:10px 0px}
.padding_20pv{padding:20px 0px}
.padding_20sl{padding:20px 0 0 20px}
.text_center{text-align:center}
.project_header{padding:0px 10px;color:#fff;font-size:1.4em}
.icon_novel{background:url("../img/titleicon/icon_traffic.png") no-repeat;padding:24px 0 21px 70px}
.icon_router{background:url("../img/titleicon/icon_router.png") no-repeat;padding:24px 0 21px 70px}
.icon_log{background:url("../img/titleicon/icon_log.png") no-repeat;padding:24px 0 21px 70px}
.media-heading{font-size:1.2em;margin-bottom:0px}
.media-heading a{color:#333}
.project_list_panel{font-size:0.9em;background:#fff}
.project_list_panel .media{padding:10px;margin:0px;border-bottom:1px solid #fff}
.project_list_panel .mediasmall{padding:5px;margin:0px;border-bottom:1px solid #fff}
.project_list_panel img{width:40px;height:40px;padding:2px;border:1px solid #ccc}
.footer{background:#222;margin-top:60px;padding:10px 0px}
.footer .container .avatar{width:48px;margin-right:10px;float:left;border:1px solid #aaa;padding:4px}